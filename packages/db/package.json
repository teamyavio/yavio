{
  "name": "@yavio/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js",
      "require": "./dist/schema.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    },
    "./rls": {
      "types": "./dist/rls.d.ts",
      "import": "./dist/rls.js",
      "require": "./dist/rls.cjs"
    },
    "./clickhouse": {
      "types": "./dist/clickhouse-client.d.ts",
      "import": "./dist/clickhouse-client.js",
      "require": "./dist/clickhouse-client.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "test": "vitest run",
    "migrate:postgres": "tsx --env-file=../../.env src/migrate.ts",
    "migrate:clickhouse": "tsx --env-file=../../.env src/migrate-clickhouse.ts",
    "db:generate": "dotenv -e ../../.env -- drizzle-kit generate",
    "db:studio": "dotenv -e ../../.env -- drizzle-kit studio"
  },
  "dependencies": {
    "@clickhouse/client": "^1.8.1",
    "@yavio/shared": "workspace:*",
    "drizzle-orm": "^0.38.4",
    "postgres": "^3.4.5"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "drizzle-kit": "^0.30.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
